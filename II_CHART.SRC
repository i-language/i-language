{w_v((1==help?"":"4\t")+upper_name_s(s1_si)+(1==help?"の種類:":"\t")+(z_p.e3s_b(z_p.upper_s(s1_si),"FONT","CUSTOMPROPERTIES")?s2_si:upper_name_s(s2_si))+(1==help?"":"\t\t\t"));
}
public void w_v(String s1_si,String s2_si,String s3_si,String s4_si,String s5_si)//引数5個
{if(1==help)w_v(" "+("$"==s1_si?"〇"+s2_si:(""==s1_si?"":s1_si+".")+s2_si+("."==s4_si?".":"=")+s3_si)+("#"==s4_si?"の種類":"")+" ----- "+s5_si);
 else{w_v(no_s+"\t"+class_s+"\t"+s2_si+"\t"+s3_si+"\t"+s4_si+"\t"+s5_si);}
}
