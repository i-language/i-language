public static String upper_name_s(String si)
  {String s="";
   for(int i=0;z_p.length(si)>i;i++)
     {if(0<i)//１文字目はそのまま
        {if(z_p.e2s_b(z_p.substring3_s(si,i,2),"3D")){s+="_3D";i++;continue;}
         if('A'<=si[i]&&'Z'>=si[i])s+='_';
        }
      s+=z_p.upper_s(""+si[i]);
     }
   return s;
  }
